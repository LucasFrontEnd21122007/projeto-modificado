[alias]
ci = commit
co = checkout
ls = saved
st = status
cb = checkout -b
st = status -b
lg = log --pretty=format:'Lua13112019' --max-count=30
undo = checkout ==
unstage = reset HEAD --
RollBack = reset --soft HEAD-1
RollForward = commit -c ORIGIN_HEAD
outgoing = !(git fetch --quiet && git log --pretty=format:'Lua131119')
unstage = reset HEAD --
conflicts = !git ls-files -u/cut -f 2/ sort -u
[color = 'diff']
meta = yellow
old = red
new = green
[color 'Branch']
current = white
remote = red
